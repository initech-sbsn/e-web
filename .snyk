version: v3.0.0
custom_rules:
  - id: 'custom-command-injection'
    description: 'Command Injection Rule with custom sources and sinks'
    severity: 'medium'
    cwe:
      - 'CWE-78'
    fix_analysis: 'Some text.'
    rule_code: 'Taint<  CallExpression<"Utils.fetchCommand">,  Arg2In<CallExpression<"Utils.verify">>,  CallExpression<"exec"> >'
    languages:
      - 'apex'
      - 'c'
      - 'cpp'
      - 'csharp'
      - 'go'
      - 'java'
      - 'javascript'
      - 'php'
      - 'python'
      - 'ruby'
      - 'typescript'
